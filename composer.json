    "psr-4": {
      "Konfigurator\\Common\\Test\\": "test"
    }
  },
  "require": {
    "php": ">=7.4.2",
    "psr/log": "^1.1",
    "amphp/amp": "^2.4",
    "myclabs/php-enum": "^1.7",
    "symfony/polyfill-php80": "^1.18"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "config": {
    "platform": {
      "php": "7.4.2"
    }
  },
  "scripts": {
    "test": "@php -dzend.assertions=1 -dassert.exception=1 ./vendor/bin/phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

{
  "name": "konfigurator/common",
  "homepage": "https://github.com/Nicodinus/Konfigurator-Common",
  "description": "",
  "type": "lib",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicodinus",
      "email": "nicodinus@yandex.ru"
    }
  ],
  "autoload": {
    "psr-4": {
      "Konfigurator\\Common\\": "lib"
    },
    "files": ["lib/functions.php"]
